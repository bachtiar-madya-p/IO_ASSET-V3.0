<?xml version="1.0" encoding="utf-8"?>

    <!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>^/login$</from>
		<to>/login.jsp</to>
	</rule>

	<rule>
		<from>^/index$</from>
		<to>/WEB-INF/view/index.jsp</to>
	</rule>

	<rule>
		<from>^/settings-governance</from>
		<to>/WEB-INF/view/settings-governance.jsp</to>
	</rule>

</urlrewrite>